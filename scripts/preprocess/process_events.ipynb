    "from bs4 import BeautifulSoup/n",
    "/n",
    "# Cleans markup from a text string/n",
    "def clean(markup_text):/n",
    "    if pd.isna(markup_text) or not isinstance(markup_text, str) or not markup_text.strip():/n",
    "        return /"/"/n",
    "    return BeautifulSoup(markup_text, /"html.parser/").get_text(separator=/" /", strip=True)/n",
    "/n",
    "# Applies markup cleanup to all object-type (string-like) columns in the Dataframe/n",
    "def clean_dataframe(df):/n",
    "    for col in df.select_dtypes(include=/"object/").columns:/n",
    "        df[col] = df[col].apply(clean)/n",
    "    return df/n",
    "/n",
    "df = clean_dataframe(df)/n",
    "/n",
    "df.head(1)"
   ]
